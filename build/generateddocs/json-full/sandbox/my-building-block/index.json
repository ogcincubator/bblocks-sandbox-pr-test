  "annotatedSchema": "$schema: https://json-schema.org/draft/2020-12/schema\ndescription: Schema for my building block\ntype: object\n$defs:\n  test:\n    type: string\nproperties:\n  a:\n    type: string\n    format: uri\n    x-jsonld-id: https://example.org/my-bb-model/a\n    x-jsonld-type: '@id'\n  b:\n    type: number\n    x-jsonld-id: https://example.org/my-bb-model/b\n  c:\n    $ref: https://raw.githubusercontent.com/ogcincubator/bblocks-sandbox-pr-test/undefined/build/annotated/sandbox/my-building-block/schema.yaml#/$defs/test\n  d:\n    $ref: https://raw.githubusercontent.com/ogcincubator/bblocks-sandbox-pr-test/undefined/build/annotated/sandbox/enumerations/legalStatus/schema.yaml\nrequired:\n- a\n- b\n",
  "semanticUplift": {
    "additionalSteps": [
      {
        "type": "jq",
        "code": ".b |= . * 66",
        "stage": "pre"
      },
      {
        "type": "jq",
        "code": ".b |= . / 11",
        "stage": "pre"
      },
      {
        "type": "sparql-construct",
        "code": "PREFIX m: <https://example.org/my-bb-model/>\nCONSTRUCT {\n  ?s ?p ?o .\n  ?s m:aalias ?a\n} WHERE { \n  ?s ?p ?o ; \n    m:a ?a \n}\n",
        "stage": "post"
      },
      {
        "type": "sparql-update",
        "code": "PREFIX m: <https://example.org/my-bb-model/>\nDELETE { ?s m:b ?b }\nWHERE { ?s m:b ?b FILTER(?b > 250) }\n",
        "stage": "post"
      },
